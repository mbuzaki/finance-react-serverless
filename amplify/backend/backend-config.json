{
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"type": "cicd"
		}
	},
	"function": {
		"plaidfunction": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": []
		},
		"plaidaccesstoken": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": []
		},
		"storeAccessToken": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "UserDB",
					"attributes": [
						"Name",
						"Arn"
					],
					"attributeEnvMap": {
						"Name": "TABLE_NAME",
						"Arn": "TABLE_ARN"
					}
				}
			]
		}
	},
	"api": {
		"plaidapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "plaidfunction",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"plaidaccessapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "plaidaccesstoken",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"addAccessToken": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "storeAccessToken",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"storage": {
		"UserDB": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	}
}