{
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"type": "cicd"
		}
	},
	"function": {
		"plaidfunction": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": []
		}
	},
	"api": {
		"plaidapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "plaidfunction",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}