{
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"type": "cicd"
		}
	},
	"function": {
		"plaid": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": []
		},
		"pyLambda": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": []
		}
	},
	"api": {
		"plaidapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "plaid",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"pyapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "pyLambda",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}